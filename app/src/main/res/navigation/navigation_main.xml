<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_main"
    app:startDestination="@id/nav_graph_splash">

    <include app:graph="@navigation/navigation_splash" />

    <include app:graph="@navigation/navigation_onboarding" />

    <include app:graph="@navigation/navigation_home" />

    <action
        android:id="@+id/action_module_to_onboarding"
        app:destination="@id/nav_graph_onboarding" />

    <action
        android:id="@+id/action_module_to_home"
        app:destination="@id/nav_graph_home" />
</navigation>